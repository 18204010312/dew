profiles:
  # 添加测试环境
  test:
    # 指定测试环境的namespace
    namespace: dew-test
    app:
      serverConfig: |-
          server {
            listen       80;
            server_name  localhost;
            location / {
              root   /usr/share/nginx/html;
              index  index.html index.htm;
            }
            if ( $http_host ~* "^(.*)") {
              set $domain $1;
              rewrite ^(.*) http://www.baidu.com break;
            }
          }